{
  "name": "react-parcel",
  "scripts": {
    "start": "parcel ./src/frontend/index.html --out-dir ./docs --public-url / --port 1145",
    "prebuild": "cd ./docs && find . -maxdepth 1 -type f -exec rm -f {} +",
    "build": "parcel build ./src/frontend/index.html --out-dir ./docs --public-url / --no-source-maps"
  },
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "parcel-bundler": "^1.12.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  }
}
